<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-init-method="turnOnLights" default-destroy-method="turnOffLights">
	
	<bean id="sax"  class="classes.Saxophone"/>	
	<bean id="piano"  class="classes.Piano"/>	
	<bean id="duke" class="classes.Juggler" scope="prototype" />
	<bean id="sonnet29" class="classes.Sonnet29" />
	<bean id="superDuke" class="classes.Juggler">
		<constructor-arg value="15" />
	</bean>

	<bean id="poeticDuke" class="classes.PoeticJuggler">
		<constructor-arg value="15" />
		<constructor-arg ref="sonnet29" />
	</bean>

	<bean id="theStage" class="classes.Stage" factory-method="getInstance" />

	<bean id="auditorium" class="classes.Auditorium" />

	<bean id="kenny" class="classes.Instrumentalist">
		<property name="age" value="25" />
		<property name="song" value="Kenny the best android in the all world!!!" />
		<property name="instrument" ref = "piano"/>
	</bean>
	
	<bean id="jenny" class="classes.Instrumentalist">
		<property name="age" value="25" />
		<property name="song" value="Kenny the best android in the all world!!!" />
		<property name="instrument" ref = "piano"/>
	</bean>
	
</beans>
